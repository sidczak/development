<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
        <meta name="robots" content="noindex,nofollow" />
        
        <!-- znacznik meta dla responsive -->
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        
        <title>{% block title %}404 Error{% endblock %}</title>
        <link rel="shortcut icon" href="{{ asset('xfavicon.ico') }}" />
        <link href="{{ asset('bundles/applicationtools/css/reset.css') }}" rel="stylesheet" type="text/css" media="all" />
        <link href="{{ asset('bundles/applicationtools/css/text.css') }}" rel="stylesheet" type="text/css" media="all" />
        <link href="{{ asset('bundles/applicationtools/css/layout.css') }}" rel="stylesheet" type="text/css" media="all" />
        <link href="{{ asset('bundles/applicationtools/css/nav.css') }}" rel="stylesheet" type="text/css" media="all" />
        <link href="{{ asset('bundles/applicationtools/css/grid_fluid.css') }}" rel="stylesheet" type="text/css" media="all" />
        <link href="{{ asset('bundles/applicationtools/css/34gs.css') }}" rel="stylesheet" type="text/css" media="all" />
        <link href="{{ asset('bundles/applicationtools/css/34gs-queries.css') }}" rel="stylesheet" type="text/css" media="all" />
        
        <!--[if IE 6]><link href="{{ asset('bundles/applicationtools/css/ie6.css') }}" rel="stylesheet" type="text/css" media="all" /><![endif]-->
        <!--[if IE 7]><link href="{{ asset('bundles/applicationtools/css/ie.css') }}" rel="stylesheet" type="text/css" media="all" /><![endif]-->
        {% block stylesheets %}

        {% endblock %}

        {% block jquery_version %}
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js" type="text/javascript"></script>
        {% endblock %}

        {% block javascripts %}
            
        {% endblock %}
        

    </head>
    <body>
        <div class="container_12">
    
                {% block body %}
                    {#
                        {% block content_header %}{% endblock %}
                        {% block content %}{% endblock %} 
                    #}
                {% endblock %}
                
            <div class="clear"></div>
            
        </div>

    </body>
</html>