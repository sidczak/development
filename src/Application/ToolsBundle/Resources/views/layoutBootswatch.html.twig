<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
        <meta name="robots" content="noindex,nofollow" />
        
        <!-- znacznik meta dla responsive -->
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        
        <title>{% block title %}Bootswatch{% endblock %}</title>
        <link rel="shortcut icon" href="{{ asset('xfavicon.ico') }}" />
        
        {% block stylesheets %}
            <link href="{{ asset('bundles/applicationtools/css/bootstrap/bootstrap.css') }}" rel="stylesheet" type="text/css" media="all" />
        {% endblock %}
        
        <link href="{{ asset('bundles/applicationtools/css/bootswatch/bootswatch.css') }}" rel="stylesheet" type="text/css" media="all" /> 
        
{#
        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
            <script src="{{ asset('bundles/applicationtools/js/assets/html5shiv.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/applicationtools/js/assets/respond.min.js') }}" type="text/javascript"></script>
        <![endif]-->
#}

        {% block jquery_version %}
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js" type="text/javascript"></script>
        {% endblock %}
        
        <script src="{{ asset('bundles/applicationtools/js/bootstrap/bootstrap.js') }}" type="text/javascript"></script>

        {% block javascripts %}{% endblock %}
        
    </head>
    <body>
        <div class="container_12">
    
                {% block body %}{% endblock %}
                
            <div class="clear"></div>
        </div>
        
        <script src="{{ asset('bundles/applicationtools/js/bootswatch/bootswatch.js') }}" type="text/javascript"></script>
    </body>
</html>