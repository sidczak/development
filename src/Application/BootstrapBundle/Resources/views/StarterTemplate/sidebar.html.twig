{% extends 'ApplicationBootstrapBundle::layout.html.twig' %}

{% block title %}Round About - Start Bootstrap Template{% endblock %}
{% block layoutstyle %}
    <link href="{{ asset('bundles/applicationbootstrap/css/examples/navbar_fixed_sticky_footer.css') }}" rel="stylesheet" type="text/css" media="all" />
{% endblock %}
{% block stylesheets %}
    <link href="{{ asset('bundles/applicationbootstrap/css/starter-template/pricing.css') }}" rel="stylesheet" type="text/css" media="all" /> 
{% endblock %}

{% block javascripts %}
{% endblock %}

{% block navigation %}
    <!-- Fixed navbar -->
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
        
            {% include 'ApplicationBootstrapBundle::_navbar.html.twig' %}
        
        </div>
    </nav>
{% endblock %}

{% block body %}
    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Sidebar Page
                    <small>Subheading</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="{{ path('application_bootstrap_start') }}">Home</a>
                    </li>
                    <li class="active">Sidebar Page</li>
                </ol>
            </div>
        </div>
        <!-- /.row -->

        <!-- Content Row -->
        <div class="row">
            <!-- Sidebar Column -->
            <div class="col-md-3">
                <div class="list-group">
                    <a href="{{ path('application_bootstrap_start') }}" class="list-group-item">Home</a>
                    <a href="{{ path('_starter-template_about') }}" class="list-group-item">About</a>
                    <a href="{{ path('_starter-template_services') }}" class="list-group-item">Services</a>
                    <a href="{{ path('_starter-template_contact') }}" class="list-group-item">Contact</a>
                    <a href="{{ path('_portfolio_portfolio-col1') }}" class="list-group-item">1 Column Portfolio</a>
                    <a href="{{ path('_portfolio_portfolio-col2') }}" class="list-group-item">2 Column Portfolio</a>
                    <a href="{{ path('_portfolio_portfolio-col3') }}" class="list-group-item">3 Column Portfolio</a>
                    <a href="{{ path('_portfolio_portfolio-col4') }}" class="list-group-item">4 Column Portfolio</a>
                    <a href="{{ path('_portfolio_portfolio-item') }}" class="list-group-item">Single Portfolio Item</a>
                    <a href="{{ path('_blog_blog-home-one') }}" class="list-group-item">Blog Home 1</a>
                    <a href="{{ path('_blog_blog-home-two') }}" class="list-group-item">Blog Home 2</a>
                    <a href="{{ path('_blog_blog-post') }}" class="list-group-item">Blog Post</a>
                    <a href="{{ path('_starter-template_full-width-page') }}" class="list-group-item">Full Width Page</a>
                    <a href="{{ path('_starter-template_sidebar') }}" class="list-group-item active">Sidebar Page</a>
                    <a href="{{ path('_starter-template_FAQ') }}" class="list-group-item">FAQ</a>
                    <a href="{{ path('_starter-template_error-404') }}" class="list-group-item">404</a>
                    <a href="{{ path('_starter-template_pricing') }}" class="list-group-item">Pricing Table</a>
                </div>
            </div>
            <!-- Content Column -->
            <div class="col-md-9">
                <h2>Section Heading</h2>
                <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Soluta, et temporibus, facere perferendis veniam beatae non debitis, numquam blanditiis necessitatibus vel mollitia dolorum laudantium, voluptate dolores iure maxime ducimus fugit.</p>
            </div>
        </div>
        <!-- /.row -->

        <hr/>

    </div> <!-- /container -->
{% endblock %}