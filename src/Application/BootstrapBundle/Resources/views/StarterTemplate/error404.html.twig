{% extends 'ApplicationBootstrapBundle::layout.html.twig' %}

{% block title %}Round About - Start Bootstrap Template{% endblock %}
{% block layoutstyle %}
    <link href="{{ asset('bundles/applicationbootstrap/css/examples/navbar_fixed_sticky_footer.css') }}" rel="stylesheet" type="text/css" media="all" />
{% endblock %}
{% block stylesheets %}
    <link href="{{ asset('bundles/applicationbootstrap/css/starter-template/pricing.css') }}" rel="stylesheet" type="text/css" media="all" /> 
{% endblock %}

{% block javascripts %}
{% endblock %}

{% block navigation %}
